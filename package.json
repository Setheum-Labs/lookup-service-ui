{
  "private": true,
  "name": "lookup-service-ui",
  "description": "Quickly check tokens & badges of addresses.",
  "version": "0.1.0",
  "keywords": [
    "ethereum",
    "parity",
    "tokens",
    "badges"
  ],
  "author": "Ethcore Team <admin@ethcore.io>",
  "maintainers": [
    "Jannis R <mail@jannisr.de>"
  ],
  "homepage": "http://ethcore.github.io/lookup-service-ui/",
  "repository": "ethcore/lookup-service-ui",
  "bugs": "https://github.com/ethcore/lookup-service-ui/issues",
  "license": "ISC",
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "envify": "^4.0.0",
    "es2020": "^1.1.9",
    "uglify-js": "^2.7.5",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "bundle": "browserify -t envify -t es2020 index.js > bundle.js",
    "minify": "cat bundle.js | uglifyjs -mc > bundle.min.js",
    "build": "npm run bundle && npm run minify",
    "start": "watchify -t envify -t es2020 index.js -o bundle.js"
  },
  "dependencies": {
    "global": "^4.3.1",
    "yo-yo": "^1.3.1"
  }
}
